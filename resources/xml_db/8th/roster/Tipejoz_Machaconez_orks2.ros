            <profile id="43ed-f88f-fe67-90fd::cc66-7ec4-b03a-8121" name="Waaagh! Energy" book="Codex: Orks" page="88" hidden="false" profileTypeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" profileTypeName="Abilities">
