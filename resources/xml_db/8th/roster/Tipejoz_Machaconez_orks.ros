              </characteristics>
            </profile>
            <profile id="43b8-74a7-9ff7-de0d::7ee3-3228-2b35-b598" name="Stormboyz Strike" hidden="false" profileTypeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" profileTypeName="Abilities">
