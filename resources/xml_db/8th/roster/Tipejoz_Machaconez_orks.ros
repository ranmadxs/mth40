                <characteristic name="Description" characteristicTypeId="21befb24-fc85-4f52-a745-64b2e48f8228" value="Add 1 to the hit rolls for models in this unit while this unit includes 20 or more models."/>
              </characteristics>
            </profile>
          </profiles>
          <selections>
            <selection id="f9d5-3807-66dd-624b" name="Gretchin" entryId="1c7b-ea7d-7177-2010::768a-467a-24bf-f0fd" entryGroupId="1c7b-ea7d-7177-2010::2f60-e906-92f6-e66a" number="10" type="model">
