            <profile id="43b8-74a7-9ff7-de0d::4217-1b88-d6e7-35c5" name="Cybork Body" hidden="false" profileTypeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" profileTypeName="Abilities">
