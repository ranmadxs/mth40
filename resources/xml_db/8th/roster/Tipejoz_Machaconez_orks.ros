              <characteristics>
                <characteristic name="Description" characteristicTypeId="21befb24-fc85-4f52-a745-64b2e48f8228" value="Each time this model loses a wound, roll a D6; on a roll of 5+, that wound is not lost.  You cannot make a Dok&apos;s Tools roll for this model if you do so."/>
              </characteristics>
            </profile>
            <profile id="43b8-74a7-9ff7-de0d::80f1-0297-40d0-3219" name="Full Throttle" hidden="false" profileTypeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" profileTypeName="Abilities">
              <profiles/>
              <rules/>
              <infoLinks/>
              <modifiers/>
              <characteristics>
                <characteristic name="Description" characteristicTypeId="21befb24-fc85-4f52-a745-64b2e48f8228" value="When Boss Zagstruk Advances you can add 6&quot; to the move ratherthan D6, roll a D6 after.   On a roll of 1, he suffers a Mortal Wound. "/>
              </characteristics>
            </profile>
            <profile id="43b8-74a7-9ff7-de0d::4344-9570-6753-0234" name="Drill Boss" hidden="false" profileTypeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" profileTypeName="Abilities">
