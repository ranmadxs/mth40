              </characteristics>
            </profile>
            <profile id="49f4-083c-a5ec-87f5::f604-0d4d-40ad-6bf4" name="Grot Orderly" hidden="false" profileTypeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" profileTypeName="Abilities">
